{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "esnext",
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": true,
    "outDir": "build",
    "composite": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "preserveSymlinks": false
  }
}
